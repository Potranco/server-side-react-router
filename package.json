{
  "name": "server-express",
  "version": "1.0.0",
  "description": "Server Express",
  "main": "server.js",
  "dependencies": {
    "babel-loader": "^7.1.2",
    "babel-node": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "express": "^4.16.2",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-router-dom": "^4.2.2"
  },
  "scripts": {
    "start": "npm run bundle-client && npm run bundle-server && node ./server/index.js",
    "bundle-server": "babel-node ./server/server.js --out-file ./server/index.js",
    "bundle-client": "webpack --progress --colors",
    "dev-server": "npm run bundle-server && nodemon ./server/index.js",
    "dev-client": "webpack-dev-server --watch --progress --colors --open"
  },
  "author": "Potranco",
  "license": "ISC",
  "devDependencies": {
    "nodemon": "^1.12.1",
    "webpack-dev-server": "^2.9.4"
  }
}
